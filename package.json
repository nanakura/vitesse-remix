{
  "name": "remix-template-remix",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "dev": "run-p dev:*",
    "build": "run-s build:*",
    "start": "remix-serve build",
    "build:css": "npm run generate:css",
    "build:remix": "remix build",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "remix dev",
    "generate:css": "unocss \"app/**/*.tsx\" -c unocss.config.ts -o ./app/styles/uno.css",
    "test": "vitest"
  },
  "dependencies": {
    "@remix-run/node": "^1.5.1",
    "@remix-run/react": "^1.5.1",
    "@remix-run/serve": "^1.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "usehooks-ts": "^2.5.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.0",
    "@iconify-json/carbon": "^1.1.5",
    "@remix-run/dev": "^1.5.1",
    "@remix-run/eslint-config": "^1.5.1",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "@unocss/cli": "^0.37.1",
    "@vitejs/plugin-react": "^1.3.2",
    "eslint": "^8.15.0",
    "happy-dom": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.6.4",
    "unocss": "^0.37.1",
    "unplugin-auto-import": "^0.8.6",
    "vite": "^2.9.9",
    "vite-tsconfig-paths": "^3.5.0",
    "vitest": "^0.13.0"
  },
  "engines": {
    "node": ">=14"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}
